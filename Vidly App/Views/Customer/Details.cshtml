@model Vidly_App.Models.Customer
@{
    ViewData["Title"] = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@Model.Name</h1>
<ul>
    <li>Membership Type: @Model.MembershipType.Name</li>
    @if(@Model.Birthday.HasValue)
    {
        <li>Birthday: @Model.Birthday</li>
    }
</ul>

